{
  "compilerOptions": {
    "declaration": true,
    "lib": ["dom", "es6"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "outDir": "web",
    "removeComments": true,
    "rootDir": "web",
    "sourceMap": false,
    "strict": true,
    "target": "ES5"
  },
  "exclude": ["node_modules"]
}
